    "key_concepts": {
      "fundamental_definitions": [
        {
          "term": "Linear System",
          "definition": "Ax = b where A is coefficient matrix, x is unknown vector, b is right-hand side"
        },
        {
          "term": "Linear Combination", 
          "definition": "Sum of scalar multiples of vectors"
        },
        {
          "term": "Non-singular Matrix",
          "definition": "Matrix whose columns span the entire space (always has solution)"
        },
        {
          "term": "Singular Matrix",
          "definition": "Matrix whose columns don't span entire space (some b have no solution)"
        }
      ],
      "three_perspectives": [
        {
          "name": "Row Picture",
          "description": "Each equation as geometric object (line in 2D, plane in 3D)"
        },
        {
          "name": "Column Picture", 
          "description": "Linear combination of column vectors",
          "importance": "most_important"
        },
        {
          "name": "Matrix Form",
          "description": "Compact notation Ax = b"
        }
      ],
      "critical_insights": [
        "Matrix-vector multiplication Ax produces linear combinations of A's columns",
        "Solvability depends on whether column combinations can produce any right-hand side b", 
        "Visualization becomes difficult beyond 3D, but mathematical principles remain consistent"
      ]
